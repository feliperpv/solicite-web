<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>erro</from-outcome>
            <to-view-id>/erro.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>usuario</from-outcome>
            <to-view-id>/usuario/inicial.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>prefeitura</from-outcome>
            <to-view-id>/prefeitura/inicialPrefeitura.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/erro.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/prefeitura/inicialPrefeitura.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pendentes</from-outcome>
            <to-view-id>/prefeitura/pendentes.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>inicial</from-outcome>
            <to-view-id>/prefeitura/inicialPrefeitura.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/usuario/inicial.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inicial</from-outcome>
            <to-view-id>/usuario/inicial.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>novasolicitacao</from-outcome>
            <to-view-id>/usuario/novasolicitacao.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>solicitacoes</from-outcome>
            <to-view-id>/usuario/solicitacoesusuario.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/prefeitura/pendentes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inicial</from-outcome>
            <to-view-id>/prefeitura/inicialPrefeitura.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/usuario/novasolicitacao.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inicial</from-outcome>
            <to-view-id>/usuario/inicial.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/usuario/solicitacoesusuario.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inicial</from-outcome>
            <to-view-id>/usuario/inicial.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
